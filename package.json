{
  "name": "vue-playground",
  "version": "0.0.0",
  "engines": {
    "node": "12.x.x",
    "npm": "6.x.x"
  },
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git@github.com:stefanfrede/vuejs-playground.git"
  },
  "keywords": [
    "vuejs",
    "vite",
    "tailwindcss"
  ],
  "author": {
    "name": "Stefan Frede",
    "email": "stefan@frede.dev"
  },
  "private": true,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stefanfrede/vuejs-playground/issues"
  },
  "homepage": "https://vuejs-spielwiese.netlify.app/",
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{css,js,vue}": "stylelint",
    "*.{js,vue}": "eslint --fix"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ext .js --ext .vue src/",
    "lint:stylelint": "stylelint src/**/*.css src/**/*.js src/**/*.vue",
    "validate": "run-s build lint",
    "setup": "npm install && npm run validate"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@vitejs/plugin-vue": "^1.0.5",
    "@vue/compiler-sfc": "^3.0.5",
    "babel-eslint": "^10.1.0",
    "browserslist": "^4.16.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.4.1",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "tailwindcss": "^2.0.2",
    "vite": "^2.0.0-beta.21"
  },
  "dependencies": {
    "vue": "^3.0.5"
  }
}
