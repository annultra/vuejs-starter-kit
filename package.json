{
  "name": "vue-playground",
  "version": "0.0.0",
  "engines": {
    "node": "12.x.x",
    "npm": "6.x.x"
  },
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git@github.com:stefanfrede/vuejs-playground.git"
  },
  "keywords": [
    "vuejs",
    "vite",
    "tailwindcss"
  ],
  "author": {
    "name": "Stefan Frede",
    "email": "stefan@frede.dev"
  },
  "private": true,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stefanfrede/vuejs-playground/issues"
  },
  "homepage": "https://vuejs-spielwiese.netlify.app/",
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{css,js,vue}": "stylelint",
    "*.{js,vue}": "eslint --fix"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ext .js --ext .vue src/",
    "lint:stylelint": "stylelint src/**/*.css src/**/*.js src/**/*.vue",
    "validate": "run-s build lint",
    "setup": "npm install && npm run validate"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@vue/compiler-sfc": "^3.0.2",
    "babel-eslint": "^10.1.0",
    "browserslist": "^4.14.5",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "tailwindcss": "^1.9.6",
    "vite": "^1.0.0-rc.8"
  },
  "dependencies": {
    "vue": "^3.0.2"
  }
}
